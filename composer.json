{
  "name": "jascha030/template",
  "description": "Template repo for creating composer projects",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Jascha030",
      "email": "contact@jaschavanaalst.nl"
    }
  ],
  "keywords": [],
  "homepage": "https://github.com/jascha030",
  "config": {
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true
    }
  },
  "autoload": {
    "psr-4": {}
  },
  "autoload-dev": {
    "psr-4": {}
  },
  "repositories": [],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.4|^8.0.0|^8.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": "4.4||^5.4||^6.0",
    "bamarni/composer-bin-plugin": "^1.8"
  },
  "extra": {

  },
  "scripts": {
    "phpunit": "phpunit -c .phpunit.xml",
    "php-cs-fixer": "php-cs-fixer fix --config .php-cs-fixer.php",
    "test": "@phpunit",
    "format": "@php-cs-fixer"
  },
  "scripts-descriptions": {
    "test": "Runs phpunit with .phpunit.xml",
    "format": "Runs php-cs-fixer with .php-cs-fixer.php."
  }
}
